{
    "name": "syn/laravel-swagger-json-api-generator",
    "version": "0.1.5",
    "description": "The package implements automatic generation of the Open Api specification for the laravel-json-api package",
    "type": "library",
    "require": {
        "php": "^8.1",
        "laravel-json-api/laravel": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "*"
    },
    "license": "GPL-3.0-only",
    "autoload": {
        "psr-4": {
            "Syn\\LaravelSwaggerJsonApiGenerator\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "SKolosov",
            "email": "skolosov.work@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Syn\\LaravelSwaggerJsonApiGenerator\\Providers\\LaravelSwaggerJsonApiGeneratorServiceProvider"
            ]
        }
    }
}
